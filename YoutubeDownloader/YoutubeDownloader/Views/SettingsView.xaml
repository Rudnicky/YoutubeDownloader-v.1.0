<UserControl
    x:Class="YoutubeDownloader.SettingsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:YoutubeDownloader"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="300"
    d:DesignWidth="300"
    Background="{DynamicResource GrafitColor}"
    mc:Ignorable="d">
    <Grid>
        <TabControl Style="{DynamicResource TabControlStyle}">
            <TabItem Header="TabItem" Style="{DynamicResource TabItemStyle}">
                <Label>aaaa</Label>
            </TabItem>
            <TabItem Header="TabItem2" Style="{DynamicResource TabItemStyle}">
                <Label>bbbb</Label>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>
